#include <iostream>

using namespace std;


int seq_search2(int key ,int *arr,int start,int end)
{
    bool found=false;
    int i=start;
    for(i=start;i<end;i++)
    {
        if(arr[i]== key)
        {
            found=true;
            break;
        }
    }
    if(found == true)
      return i;
    else
        return -99;

}

struct indexedArray
{
    int val;
    int slot;

};

int Index_search(int key, int*arr,int s,indexedArray idxArr[],int s_idx)
{
    int start,endd,ret;
    if(key < idxArr[0].val)
    cout<<"not found"<<endl;

    int i=0;
    while(key > idxArr[i].val && i<(s_idx-1))
    {
        i++;
    }
    if(i==s_idx-1)
    {
        start= idxArr[i].slot;
        endd=s-1;
        ret =seq_search2(key ,arr,start,endd);

    }
    else
    {
        start=idxArr[i-1].slot;
        endd =idxArr[i].slot;
        ret=seq_search2(key ,arr,start,endd);
    }

    return ret;


}


int main()
{
    int arr[13]={5,12,13,20,23,31,44,46,57,68,70,92,100};
    indexedArray idxArray[4]={{5,0},{20,3},{44,6},{68,9}};

    cout<<Index_search(1,arr,13,idxArray,4);
    return 0;
}
