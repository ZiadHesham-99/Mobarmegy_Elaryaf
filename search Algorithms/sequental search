#include <iostream>

using namespace std;



int seq_search(int key ,int *arr,int s)
{
    bool found=false;
    int i=0;
    for(i=0;i<s;i++)
    {
        if(arr[i]== key)
        {
            found=true;
            break;
        }
    }


    if(found == true)
      //cout<<"found at index "<<i;
      return i;
    else
        return -99;




}

int main()
{
    int arr[6]={1,4,5,7,8,6};
   cout<< seq_search(1,arr,6);

  
    return 0;
}
